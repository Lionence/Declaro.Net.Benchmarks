{
    "version": "2.0",
    "logging": {
      "logLevel": {
        "default": "Error",
        "Host": "Error",
        "Function": "Error",
        "Host.Aggregator": "Error"
      },
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "enableLiveMetricsFilters": true
      },
      "console": {
          "isEnabled": false,
          "DisableColors": true
        }
    }
}